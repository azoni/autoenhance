# Required — get your API key from https://app.autoenhance.ai/settings
AUTOENHANCE_API_KEY=your_api_key_here

# Optional — Require X-API-Key header on batch endpoints (leave blank for open access)
SERVICE_API_KEY=

# Optional — Comma-separated allowed CORS origins (defaults to render.com + localhost)
ALLOWED_ORIGINS=

# Optional — Admin token for protected endpoints (auto-generated per-boot if unset)
ADMIN_TOKEN=

# Optional — Sentry error tracking (leave blank to disable)
SENTRY_DSN=
SENTRY_AUTH_TOKEN=
SENTRY_ORG=
SENTRY_PROJECT=
